# some more ls aliases
alias refresh='source ~/.bashrc'
alias ls='ls -F --color=always'
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias lt='ls -lrt'
alias lat='ls -lart'
alias e='vim'
alias u='cd ../'
alias uu='cd ../../'
alias uuu='cd ../../../'

alias rm='rm -i'

alias g='git'

alias fn='find . -iname $1'
