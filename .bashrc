# some more ls aliases
alias refresh='source ~/.bashrc'
alias ls='ls -F --color=always'
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias lt='ls -lrt'
alias lat='ls -lart'
alias e='vim'
alias u='cd ../'
alias uu='cd ../../'
alias uuu='cd ../../../'

alias rm='rm -i'

# git related
#alias g='git'
alias gg='git grep -i'
alias gd='git diff'
alias gst='git status'
alias gb='git branch'
alias gl='git ls'

alias fn='find . -iname $1'
alias st='st 2>/dev/null'
